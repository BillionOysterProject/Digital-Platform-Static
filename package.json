{
  "name": "BOP-Digital-Platform-Static",
  "version": "0.0.1",
  "description": "Front end for the BOP Digital Platform.",
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist",
    "clean:dist": "rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "webpack --watch --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached --port 8081  --content-base src/",
    "server:prod": "http-server dist --cors",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "lint": "tsconfig-lint",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "ts-node node_modules/.bin/protractor --specs src/**/**.e2e.ts,src/**/*.e2e.ts",
    "e2e:live": "protractor --elementExplorer",
    "test": "karma start",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json --exclude **/*.spec.ts ./src/",
    "start": "npm run server:dev",
    "fix:rx": "node_modules/.bin/replace '5.0.0-beta.1' '5.0.0-beta.1' node_modules -r --exclude=node_modules/rxjs",
    "fix:ng2-patch": "node_modules/.bin/ncp src/.ng2-patch node_modules",
    "fix:ng2-clean": "node_modules/.bin/rimraf node_modules/angular2/manual_typings node_modules/angular2/typings node_modules/angular2/bundles/typings",
    "fix:ng2": "npm run fix:ng2-clean && npm run fix:ng2-patch && npm run fix:rx",
    "pretypings-install": "npm run fix:ng2",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BillionOysterProject/Digital-Platform-Static.git"
  },
  "keywords": [
    "BOP",
    "Billion Oyster Project"
  ],
  "engines": {
    "node": ">= 4.0 <= 5",
    "npm": ">= 3"
  },
  "author": "chris hogan <chris@hogan.io>",
  "license": "MIT",
  "devDependencies": {
    "compression-webpack-plugin": "^0.3.0",
    "copy-webpack-plugin": "^1.0.0",
    "css-loader": "^0.23.0",
    "es6-promise-loader": "^1.0.1",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "1.7.0",
    "http-server": "^0.8.5",
    "imports-loader": "^0.6.4",
    "istanbul-instrumenter-loader": "^0.1.3",
    "json-loader": "^0.5.3",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "1.7.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.0.0",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-helper": "0.0.1",
    "ts-loader": "^0.8.0",
    "ts-node": "^0.5.5",
    "tsconfig-lint": "^0.5.0",
    "tslint": "^3.2.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.5",
    "typings": "^0.6.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-md5-hash": "0.0.4"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.1",
    "angular2-jwt": "^0.1.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "es7-reflect-metadata": "^1.5.5",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.1",
    "zone.js": "^0.5.10"
  }
}
